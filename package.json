{
  "name": "@regulaforensics/document-reader-webclient",
  "source": "src/index.ts",
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "version": "6.6.0",
  "description": "Regula Document Reader js client for the browser and node.js based on axios",
  "author": "Regula Forensics, Inc.",
  "keywords": [
    "document-reader-webclient",
    "document reader",
    "document recognition",
    "regulaforensics",
    "regula"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com/regulaforensics/DocumentReader-api-js-client"
  },
  "exports": {
    "import": "./dist/module/index.js",
    "require": "./dist/main/index.cjs"
  },
  "main": "dist/main/index.cjs",
  "module": "dist/module/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "targets": {
    "main": {
      "optimize": true,
      "sourceMap": false
    },
    "module": {
      "optimize": true,
      "sourceMap": false
    },
    "types": {
      "sourceMap": false
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "format": "eslint --fix",
    "build": "parcel build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@regulaforensics/document-reader-webclient": "^6.9.4",
    "@swc/helpers": "^0.5.3",
    "axios": "^1.6.1",
    "base64-arraybuffer": "^1.0.2",
